<form id="adminform" method="post" action="/admin">
	<div>
		<fieldset>
			<h2>Ingestions</h2>
			<div>
				<div>
					<label for="<%= locals.FormID.IngestAllLibraries %>">
						ğŸ—ƒ Global Ingestion (on = All Releases)
					</label>
				</div>
				<div>
					<input type="checkbox" id="<%= locals.FormID.IngestAllLibraries %>" name="<%= locals.FormID.IngestAllLibraries %>" value="1" />
				</div>
			</div>
			<div>
				<div>
					<label for="<%= locals.FormID.IngestAccessories %>">
						ğŸ—ƒ 1. Ingest Manufacturers Wheels
					</label>
				</div>
				<div>
					<input type="checkbox" id="<%= locals.FormID.IngestAccessories %>" name="<%= locals.FormID.IngestAccessories %>" value="1" />
				</div>
			</div>
			<div>
				<div>
					<label for="<%= locals.FormID.IngestFixture %>">
						ğŸ—ƒ 2. Ingest Fixtures
					</label>
				</div>
				<div>
					<input type="checkbox" id="<%= locals.FormID.IngestFixture %>" name="<%= locals.FormID.IngestFixture %>" value="1" />
				</div>
			</div>
			<div>
				<div>
					<label for="<%= locals.FormID.UpdateFixtureAccessories %>">
						ğŸ—ƒ 3. Update Fixtures Profile with Pictures
					</label>
				</div>
				<div>
					<input type="checkbox" id="<%= locals.FormID.UpdateFixtureAccessories %>" name="<%= locals.FormID.UpdateFixtureAccessories %>" value="1" />
				</div>
			</div>
			<div>
				<div>
					<label for="<%= locals.FormID.ComputeFixtureChangelog %>">
						ğŸ—ƒ 4. Compute Fixtures Changelog
					</label>
				</div>
				<div>
					<input type="checkbox" id="<%= locals.FormID.ComputeFixtureChangelog %>" name="<%= locals.FormID.ComputeFixtureChangelog %>" value="1" />
				</div>
			</div>
			<div>
				<div>
					<label for="reparameters">
						ğŸ“’ 5. Ingest DMX Attributs
					</label>
				</div>
				<div>
					<input type="checkbox" id="reparameters" name="reparameters" value="1" />
				</div>
			</div>
			<div>
				<div>
					<label for="restats">
						ğŸ“ˆ 6. Compute FF@ Statistics
					</label>
				</div>
				<div>
					<input type="checkbox" id="restats" name="restats" value="1" />
				</div>
			</div>
			<div>
				<div>
					<label for="scanpreset">
						ğŸ—ƒ Scan Only Fixtures Presets
					</label>
				</div>
				<div>
					<input type="checkbox" id="scanpreset" name="scanpreset" value="1" />
				</div>
			</div>
		</fieldset>
		<fieldset>
			<h2>Quality</h2>
			<div>
				<div>
					<label>
						Reports
					</label>
				</div>
				<div>
					<a href="./qualityscan">Global file report</a> <a href="./quality">Fixture Quality report</a>
				</div>
			</div>
		</fieldset>
		<fieldset>
			<h2>Referencing</h2>
			<div>
				<div>
					<label for="resitemap">
						ğŸ–§ Renew Sitemap
					</label>
				</div>
				<div>
					<input type="checkbox" id="resitemap" name="resitemap" value="1" />
				</div>
			</div>
		</fieldset>
		<fieldset>
			<h2>Cache Management</h2>
			<div>
				<div>
					<label for="cleancache">
						ğŸ“ Clean all caches
					</label>
				</div>
				<div>
					<input type="checkbox" id="cleancache" name="cleancache" value="1" />
				</div>
			</div>
			<div>
				<div>
					<label for="cleancacheChangelog">
						ğŸ— Clean Changelog cache
					</label>
				</div>
				<div>
					<input type="checkbox" id="cleancacheChangelog" name="cleancacheChangelog" value="1" />
				</div>
			</div>
			<div>
				<div>
					<label for="cleancacheFixtureChangelog">
						ğŸ— Clean Fixture Changelog cache
					</label>
				</div>
				<div>
					<input type="checkbox" id="cleancacheFixtureChangelog" name="cleancacheFixtureChangelog" value="1" />
				</div>
			</div>
			<div>
				<div>
					<label for="cleancacheFixtureList">
						ğŸ— Clean Fixture list cache
					</label>
				</div>
				<div>
					<input type="checkbox" id="cleancacheFixtureList" name="cleancacheFixtureList" value="1" />
				</div>
			</div>
			<div>
				<div>
					<label for="cleancacheFixtureDetail">
						ğŸ— Clean Fixture detail cache
					</label>
				</div>
				<div>
					<input type="checkbox" id="cleancacheFixtureDetail" name="cleancacheFixtureDetail" value="1" />
				</div>
			</div>
			<div>
				<div>
					<label for="cleancacheSearch">
						ğŸ— Clean Search Cache
					</label>
				</div>
				<div>
					<input type="checkbox" id="cleancacheSearch" name="cleancacheSearch" value="1" />
				</div>
			</div>
			</fieldset>
			<fieldset>
				<h2>Security</h2>
				<div>
					<div>
						<label for="parse">
							ğŸ”’ Password
						</label>
					</div>
					<div>
						<input type="password" name="parse" id="parse"/>
					</div>
				</div>

				<div>
					<div>
						<label for="submit">
							Ready?
						</label>
					</div>
					<div>
						<input name="submit" id="submit" type="submit" value="Have some fun ..." />
					</div>
				</div>
			</fieldset>
	</div>
	<% if (locals.flash) { %>
		<div>
			<fieldset>
				<% if (locals.flash.error) { %>
				<h2 class="error"><%= locals.flash.error %><h2>
				<% } else if (locals.flash.ok) { %>
				<h2 class="ok"><%= locals.flash.ok %><h2>
				<% } if (locals.flash.content) { %>
				<p><%= locals.flash.content %><p>
				<% } %>
			</fieldset>
		</div>
	<% } %>
</form>
